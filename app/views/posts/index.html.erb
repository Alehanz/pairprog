<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <%= button_to "New Post", new_post_path, method: "get" %>
    <table class="table table-striped table-bordered table-condensed">
      <tr>
        <th>Post</th>
      </tr>
      <tbody>
        <% @posts.each do |post| %>
          <tr>
            <td>
              <%= link_to post.title, post %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <%= will_paginate @posts %>
  </div>
</div>
