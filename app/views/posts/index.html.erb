<div class="container">
<div class= "row">
  <div class="col-md-10 col-md-offset-1">
    <%= button_to "New Post", new_post_path, method: "get" %>
    <table class="table">
      <tbody>
        <% @posts.each do |post| %>
          <tr>
            <td>
              <%= link_to post.title, post %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
</div>
